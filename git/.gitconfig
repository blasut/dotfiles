[core]
  editor = emacs
  excludesfile = ~/.gitignore_global

[color]
  ui = auto
	branch = auto
	diff = auto
	status = auto

[alias]
	st = status
	ci = commit
	cm = commit -m
  co = checkout
	chkecout = checkout
	cehckout = checkout
  di = diff
  dc = diff --cached
  b = branch
  aa = add --all
  sync = pull --rebase 
  p = push
  pu = push
  psuh = push
  r = rebase
  s = pull --rebase
  f = log --follow

[format]
  pretty=format:%C(yellow)%h%Creset -%C(red)%d%Creset %s %Cgreen(%ar) %C(bold blue)<%an>%Creset

[push]
  default = simple

